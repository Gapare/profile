{
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/admin/(.*)", 
            "dest": "core/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        }
    ]
}